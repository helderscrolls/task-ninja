        "libs/mobile/tasks/data-access/src/index.ts"
      ],
      "@task-ninja/mobile/tasks/feature/add-task": [
        "libs/mobile/tasks/feature/add-task/src/index.ts"
      ],
      "@task-ninja/mobile/tasks/feature/task-detail": [
        "libs/mobile/tasks/feature/task-detail/src/index.ts"
